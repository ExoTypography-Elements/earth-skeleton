.et-table > thead > tr > td.et-danger,
.et-table > tbody > tr > td.et-danger,
.et-table > tfoot > tr > td.et-danger,
.et-table > thead > tr > th.et-danger,
.et-table > tbody > tr > th.et-danger,
.et-table > tfoot > tr > th.et-danger,
.et-table > thead > tr.et-danger > td,
.et-table > tbody > tr.et-danger > td,
.et-table > tfoot > tr.et-danger > td,
.et-table > thead > tr.et-danger > th,
.et-table > tbody > tr.et-danger > th,
.et-table > tfoot > tr.et-danger > th {
  color: #dd514c;
  background-color: rgba(221, 81, 76, 0.115);
}