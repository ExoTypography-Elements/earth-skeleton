/* ==========================================================================
   Component: Table
 ============================================================================ */
table {
  max-width: 100%;
  background-color: transparent;
  empty-cells: show;
}
table code {
  white-space: normal;
}
th {
  text-align: left;
}
.et-table {
  width: 100%;
  margin-bottom: 1.6rem;
  border-spacing: 0;
  border-collapse: separate;
}
.et-table > thead > tr > th,
.et-table > tbody > tr > th,
.et-table > tfoot > tr > th,
.et-table > thead > tr > td,
.et-table > tbody > tr > td,
.et-table > tfoot > tr > td {
  padding: 0.7rem;
  line-height: 1.6;
  vertical-align: top;
  border-top: 1px solid #ddd;
}
.et-table > thead > tr > th {
  vertical-align: bottom;
  border-bottom: 1px solid #ddd;
}
.et-table > caption + thead > tr:first-child > th,
.et-table > colgroup + thead > tr:first-child > th,
.et-table > thead:first-child > tr:first-child > th,
.et-table > caption + thead > tr:first-child > td,
.et-table > colgroup + thead > tr:first-child > td,
.et-table > thead:first-child > tr:first-child > td {
  border-top: 0;
}
.et-table > tbody + tbody tr:first-child td {
  border-top: 2px solid #ddd;
}
/* Bordered version */
.et-table-bordered {
  border: 1px solid #ddd;
  border-left: none;
}
.et-table-bordered > thead > tr > th,
.et-table-bordered > tbody > tr > th,
.et-table-bordered > tfoot > tr > th,
.et-table-bordered > thead > tr > td,
.et-table-bordered > tbody > tr > td,
.et-table-bordered > tfoot > tr > td {
  border-left: 1px solid #ddd;
  /*&:first-child {
          border-left: none;
        }*/
}
.et-table-bordered > tbody > tr:first-child > th,
.et-table-bordered > tbody > tr:first-child > td {
  border-top: none;
}
.et-table-bordered > thead + tbody > tr:first-child > th,
.et-table-bordered > thead + tbody > tr:first-child > td {
  border-top: 1px solid #ddd;
}
/* Border-radius version */
.et-table-radius {
  border: 1px solid #ddd;
  border-radius: 2px;
}
.et-table-radius > thead > tr:first-child > th:first-child,
.et-table-radius > thead > tr:first-child > td:first-child {
  border-top-left-radius: 2px;
  border-left: none;
}
.et-table-radius > thead > tr:first-child > th:last-child,
.et-table-radius > thead > tr:first-child > td:last-child {
  border-top-right-radius: 2px;
  border-right: none;
}
.et-table-radius > tbody > tr > th:first-child,
.et-table-radius > tbody > tr > td:first-child {
  border-left: none;
}
.et-table-radius > tbody > tr > th:last-child,
.et-table-radius > tbody > tr > td:last-child {
  border-right: none;
}
.et-table-radius > tbody > tr:last-child > th,
.et-table-radius > tbody > tr:last-child > td {
  border-bottom: none;
}
.et-table-radius > tbody > tr:last-child > th:first-child,
.et-table-radius > tbody > tr:last-child > td:first-child {
  border-bottom-left-radius: 2px;
}
.et-table-radius > tbody > tr:last-child > th:last-child,
.et-table-radius > tbody > tr:last-child > td:last-child {
  border-bottom-right-radius: 2px;
}
/* Zebra-striping */
.et-table-striped > tbody > tr:nth-child(odd) > td,
.et-table-striped > tbody > tr:nth-child(odd) > th {
  background-color: #f9f9f9;
}
/* Hover effect */
.et-table-hover > tbody > tr:hover > td,
.et-table-hover > tbody > tr:hover > th {
  background-color: #e9e9e9;
}
.et-table-compact > thead > tr > th,
.et-table-compact > tbody > tr > th,
.et-table-compact > tfoot > tr > th,
.et-table-compact > thead > tr > td,
.et-table-compact > tbody > tr > td,
.et-table-compact > tfoot > tr > td {
  padding: 0.4rem;
}
.et-table-centered > thead > tr > th,
.et-table-centered > tbody > tr > th,
.et-table-centered > tfoot > tr > th,
.et-table-centered > thead > tr > td,
.et-table-centered > tbody > tr > td,
.et-table-centered > tfoot > tr > td {
  text-align: center;
}
.et-table > thead > tr > td.et-active,
.et-table > tbody > tr > td.et-active,
.et-table > tfoot > tr > td.et-active,
.et-table > thead > tr > th.et-active,
.et-table > tbody > tr > th.et-active,
.et-table > tfoot > tr > th.et-active,
.et-table > thead > tr.et-active > td,
.et-table > tbody > tr.et-active > td,
.et-table > tfoot > tr.et-active > td,
.et-table > thead > tr.et-active > th,
.et-table > tbody > tr.et-active > th,
.et-table > tfoot > tr.et-active > th {
  background-color: #ffd;
}
.et-table > thead > tr > td.et-disabled,
.et-table > tbody > tr > td.et-disabled,
.et-table > tfoot > tr > td.et-disabled,
.et-table > thead > tr > th.et-disabled,
.et-table > tbody > tr > th.et-disabled,
.et-table > tfoot > tr > th.et-disabled,
.et-table > thead > tr.et-disabled > td,
.et-table > tbody > tr.et-disabled > td,
.et-table > tfoot > tr.et-disabled > td,
.et-table > thead > tr.et-disabled > th,
.et-table > tbody > tr.et-disabled > th,
.et-table > tfoot > tr.et-disabled > th {
  color: #999999;
}
.et-table > thead > tr > td.et-primary,
.et-table > tbody > tr > td.et-primary,
.et-table > tfoot > tr > td.et-primary,
.et-table > thead > tr > th.et-primary,
.et-table > tbody > tr > th.et-primary,
.et-table > tfoot > tr > th.et-primary,
.et-table > thead > tr.et-primary > td,
.et-table > tbody > tr.et-primary > td,
.et-table > tfoot > tr.et-primary > td,
.et-table > thead > tr.et-primary > th,
.et-table > tbody > tr.et-primary > th,
.et-table > tfoot > tr.et-primary > th {
  color: #0b76ac;
  background-color: rgba(14, 144, 210, 0.115);
}
.et-table > thead > tr > td.et-success,
.et-table > tbody > tr > td.et-success,
.et-table > tfoot > tr > td.et-success,
.et-table > thead > tr > th.et-success,
.et-table > tbody > tr > th.et-success,
.et-table > tfoot > tr > th.et-success,
.et-table > thead > tr.et-success > td,
.et-table > tbody > tr.et-success > td,
.et-table > tfoot > tr.et-success > td,
.et-table > thead > tr.et-success > th,
.et-table > tbody > tr.et-success > th,
.et-table > tfoot > tr.et-success > th {
  color: #5eb95e;
  background-color: rgba(94, 185, 94, 0.115);
}
.et-table > thead > tr > td.et-warning,
.et-table > tbody > tr > td.et-warning,
.et-table > tfoot > tr > td.et-warning,
.et-table > thead > tr > th.et-warning,
.et-table > tbody > tr > th.et-warning,
.et-table > tfoot > tr > th.et-warning,
.et-table > thead > tr.et-warning > td,
.et-table > tbody > tr.et-warning > td,
.et-table > tfoot > tr.et-warning > td,
.et-table > thead > tr.et-warning > th,
.et-table > tbody > tr.et-warning > th,
.et-table > tfoot > tr.et-warning > th {
  color: #F37B1D;
